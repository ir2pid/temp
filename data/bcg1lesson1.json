[
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (1)",
                        "text1": "আমি যাই",
                        "text2": "I go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (2)",
                        "text1": "আমি যাচ্ছি",
                        "text2": "I am going."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (3)",
                        "text1": "আমি যাবো",
                        "text2": "I shall go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (4)",
                        "text1": "আমি গিয়েছি",
                        "text2": "I have gone."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (5)",
                        "text1": "আমি যাই না",
                        "text2": "I do not go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (6)",
                        "text1": "আমি যাচ্ছি না",
                        "text2": "I am not going."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (7)",
                        "text1": "আমি যাবো না",
                        "text2": "I shall not go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (8)",
                        "text1": "আমি গিয়েছিলাম",
                        "text2": "I went."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (9)",
                        "text1": "আমি যাচ্ছিলাম",
                        "text2": "I was going."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (10)",
                        "text1": "আমি যাই নি",
                        "text2": "I did not go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (11)",
                        "text1": "আমি যেতে পারি",
                        "text2": "I can go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (12)",
                        "text1": "আমি যেতে পারি না",
                        "text2": "I can not go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (13)",
                        "text1": "আমি কি যাই ?",
                        "text2": "Do I go ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (14)",
                        "text1": "আমি কি যাই না ?",
                        "text2": "Do I not go ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (15)",
                        "text1": "আমি কি গিয়েছি ?",
                        "text2": "Have I gone ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (16)",
                        "text1": "আমি কি গেছি না/ গিয়েছি না ?",
                        "text2": "Have I not gone ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (17)",
                        "text1": "আমি কি গিয়েছিলাম ?",
                        "text2": "Did I go ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (18)",
                        "text1": "আমি কি যাই নি ?",
                        "text2": "Did I not go ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (19)",
                        "text1": "আমি যেতে পারি কি ?",
                        "text2": "May I go ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (20)",
                        "text1": "আমার যাওয়া উচিত",
                        "text2": "I should go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (21)",
                        "text1": "আমি কি যাবো ?",
                        "text2": "Shall I go ?"
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (22)",
                        "text1": "আমিই যাবো",
                        "text2": "I only shall go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (23)",
                        "text1": "আমায় যেতেই হবে",
                        "text2": "I must go."
                      },
                      {
                        "description": "",
                        "image1": "",
                        "name": "BCg1-Lesson1 (24)",
                        "text1": "আমি কি যাবো না ?",
                        "text2": "Shall I not go ?"
                      }
]